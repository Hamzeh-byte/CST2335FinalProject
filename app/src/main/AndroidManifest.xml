<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vogella.android.cst2335finalproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CST2335FinalProject"
        android:parentActivityName = "CovidSearchResult">
        <activity android:name=".CovidDBSave"></activity>
        <activity android:name=".CovidDBLoad" />
        <activity android:name=".CovidEmptyActivity" />
        <activity android:name=".CovidSearchResult" />
        <activity android:name=".CovidMain" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>